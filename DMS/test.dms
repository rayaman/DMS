entry main // Will either start the first block seen or the block supplied by you!
//enable warnings
disable omniscient
enable fullname
enable forwardlabels // Do most of your labels exist ahead?
enable savestate
//enable leaking
enable debugging
//loadfile "loadtest.dms"
version 0.2
using extendedDefine
[main]
	num = 30.0
	io.print("!"+num+" = " + fact(num))

[fact:function(n)]
	if(n==1)
		return 1
	else 
		return n * fact(n-1)

[this:function()]
	print("This")
	
[that:function()]
	print("That")

