entry main // Will either start the first block seen or the block supplied by you!
//enable warnings
disable omniscient
enable forseelabels
enable savestate
disable fullname
//enable leaking
enable debugging
loadfile "loadtest.dms"
version 0.2
using extendedDefine

[main]
	"Time to test function calls!"
	val = newblock(1,2,3)
	"val = `val`"

[Bob:char]
	//fname = "Bob"
	//known = true // defaults to false
	//lname = "Johnson" // defaults to ""
	unknown = "Some Random Guy"
	age = .24
	money = 100
	excited: "path/to/file"

[newblock:function(a,b,c)]
	"Func Arguments: a = `a`, b = `b`, c = `c`"
	"Time to return"
	return a+b+c
